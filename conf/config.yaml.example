gui: "127.0.0.1:9999"
use_gui: true
allow_ips: ["127.0.0.1", "192.168.1.100"]
use_embed_ui: true
proxies:
  - alias: db1
    enabled: true
    listen: "127.0.0.1:19991"
    mysql: "ip:port"
    proxy_passwd: "your_proxy_password"
  - alias: db2
    enabled: true
    listen: "127.0.0.1:19992"
    mysql: "ip:port"
    proxy_passwd: "your_proxy_password"